pub fn NotFound(ctx: zx.NotFoundContext) zx.Component {
    return (
        <div @allocator={ctx.arena}>
            <h1>404 - Page Not Found</h1>
            <p>The page you are looking for does not exist.</p>
            <a href="/">Go back to the home page</a>
        </div>
    );
}

const zx = @import("zx");
