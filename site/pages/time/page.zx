pub fn Page(ctx: zx.PageContext) zx.Component {
    const allocator = ctx.arena;
    
        const time = std.time.timestamp();

    return (
        <div @allocator={allocator}>
            <h1>Time:</h1> 
            <p>
                {time}
            </p>
        </div>
    );
}

const zx = @import("zx");
const std = @import("std");