pub fn ReactDemo(allocator: zx.Allocator) zx.Component {
    const max_count = 10;

    return (
        <main @allocator={allocator}>
            <CounterComponent @rendering={.react} max_count={max_count} />
        </main>
    );
}

const zx = @import("zx");
const CounterComponent = @jsImport("react.tsx");
