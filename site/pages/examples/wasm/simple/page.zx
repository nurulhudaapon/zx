var count: i32 = 0;
pub fn Page(ctx: zx.PageContext) zx.Component {
    count += 1;

    return (
        <div @allocator={ctx.arena}>
            <CounterComponent @rendering={.client} count={count} />
        </div>
    );
}

pub const CounterComponent = @import("../../client_counter.zx").CounterComponent;

const zx = @import("zx");