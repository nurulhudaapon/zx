pub fn Page(allocator: zx.Allocator) zx.Component {
    const @"data-name" = "hello";
    const value: i32 = 42;
    const class = "b-1 bold";

    return (
        <form @{allocator}>
            <input {@"data-name"} {class} />
            <input {value} />
        </form>
    );
}

const zx = @import("zx");
