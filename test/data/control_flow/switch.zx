pub fn Page(allocator: zx.Allocator) zx.Component {
    const user_type: UserType = .admin;
    return (
        <main @allocator={allocator}>
            {switch (user_type) {
                .admin => ("Admin"),
                .member => ("Member"),
            }}
            {switch (user_type) {
                .admin => "Admin",
                .member => "Member",
            }}
            {switch (user_type) {
                .admin => (<p>Powerful</p>),
                .member => (<p>Powerless</p>),
            }}
        </main>
    );
}

const zx = @import("zx");

const UserType = enum { admin, member };
