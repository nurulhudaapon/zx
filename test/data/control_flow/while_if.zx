pub fn Page(allocator: zx.Allocator) zx.Component {
    var i: usize = 0;

    return (
        <main @allocator={allocator}>
            {while (i < 3) : (i +=1) (
                <div>
                    {if (i % 2 == 0) (<p>Even: {[i:d]}</p>) else (<p>Odd: {[i:d]}</p>)}
                </div>
            )}
        </main>
    );
}

const zx = @import("zx");
