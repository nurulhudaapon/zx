pub fn Page(allocator: zx.Allocator) zx.Component {
    const is_logged_in = true;
    const is_premium = true;

    return (
        <main @allocator={allocator}>
            <p>Welcome, User!</p>
            {if (is_logged_in) (
                if (is_premium) (
                    <p>Welcome, Premium User!</p>
                )
            )}
        </main>
    );
}

const zx = @import("zx");
